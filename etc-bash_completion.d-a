#!/bin/bash

# Add a soft link to this to /etc/bash_completion.d/ and 
# source it from .bashrc to get autocomplete

_a() 
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    #opts="--help --verbose --version"
    opts="$( cut -d: -f1 /tmp/tumx_sess_cache.txt | cut -d_ -f5 )"

    if [[ ${cur} == * ]] ; then
        COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
        return 0
    fi
}
complete -F _a a
